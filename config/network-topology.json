      "providers": ["supplier-proxy"]
    },
    {
      "consumer": "carrier-proxy",
      "services": ["contract-negotiation"],
      "providers": ["plant-proxy"]
    },
    {
      "consumer": "plant-proxy",
      "services": ["contract-negotiation"],
      "providers": ["carrier-proxy"]
    },
    {
      "consumer": "plant-proxy",
      "services": ["contract-negotiation"],
      "providers": ["supplier-proxy"]
    },
    {
      "consumer": "supplier-proxy",
      "services": ["contract-negotiation"],
      "providers": ["carrier-proxy"]
    },
    {
      "consumer": "supplier-proxy",
      "services": ["contract-negotiation"],
      "providers": ["plant-proxy"]
    },

    {
      "consumer": "carrier-proxy",
      "services": ["event-publish"],
      "providers": ["event_handler"]
    },
    {
      "consumer": "plant-proxy",
      "services": ["event-publish"],
      "providers": ["event_handler"]
    },
    {
      "consumer": "supplier-proxy",
      "services": ["event-publish"],
