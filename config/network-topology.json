    {
      "address": "supplier-agent-forecasting.p4sc",
      "authenticationInfo": "",
      "port": 9803,
      "systemName": "supplier-agent-forecasting"
    },
    {
      "address": "supplier-agent-sales.p4sc",
      "authenticationInfo": "",
