    {
      "consumer": "carrier-agent-booking",
      "services": ["trusted-contract-negotiation", "trusted-contract-observation"],
      "providers": ["carrier-proxy"]
    },
    {
      "consumer": "plant-agent-purchasing",
      "services": ["trusted-contract-negotiation", "trusted-contract-observation"],
      "providers": ["plant-proxy"]
    },
    {
      "consumer": "supplier-agent-forecasting",
      "services": ["trusted-contract-negotiation", "trusted-contract-observation"],
      "providers": ["supplier-proxy"]
    },
    {
      "consumer": "supplier-agent-sales",
      "services": ["trusted-contract-negotiation", "trusted-contract-observation"],
      "providers": ["supplier-proxy"]
    },

    {
      "consumer": "carrier-proxy",
